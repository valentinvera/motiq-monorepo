  },
  "dependencies": {
    "@fontsource/inter": "5.2.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.523.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.4"
  },
  "devDependencies": {
    "@motiq/eslint-config": "workspace:*",
    "@motiq/ts-config": "workspace:*",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
